{
  "name": "msd-back",
  "version": "1.0.0",
  "private": true,
  "description": "msd-back",
  "author": "Quentin LE CAIGNEC",
  "keywords": [
    "react"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean": "rm -rf .cache .tmp dist build coverage",
    "lint": "npm run lint:eslint && npm run lint:style && npm run lint:prettier",
    "lint:eslint": "eslint . --fix --max-warnings=0 --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:style": "stylelint src/**/*.{css,scss,sass} --max-warnings=0 --fix",
    "lint:prettier": "prettier . --write --ignore-unknown --ignore-path .gitignore --ignore-path .prettier.ignore",
    "test": "npm run test:typecheck && npm run test:jest",
    "test:typecheck": "tsc --noEmit",
    "test:jest": "react-scripts test",
    "test:ci": "jest --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-scripts": "5.0.1",
    "sass": "1.54.4",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.18.10",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "28.1.7",
    "@types/node": "17.0.45",
    "@types/react": "18.0.16",
    "@types/react-dom": "18.0.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/parser": "5.33.0",
    "babel-jest": "28.1.3",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-webpack-plugin": "3.2.0",
    "husky": "8.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-junit": "14.0.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "stylelint": "14.10.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "27.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.7.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --max-warnings=0 --ignore-path .gitignore",
    "*.{css,scss,sass}": "stylelint src/**/*.{css,scss} --fix --max-warnings=0 --ignore-path .gitignore",
    "*": "prettier --write --ignore-unknown --ignore-path .gitignore --ignore-path .prettier.ignore"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
